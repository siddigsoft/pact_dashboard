modules = ["nodejs-20", "web", "postgresql-16"]
[agent]
expertMode = true
integrations = ["github:1.0.0", "javascript_gemini_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["imagemagick"]

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "start"]
build = ["npm", "run", "build"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev -- --port 5000 --strictPort"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[objectStorage]
defaultBucketID = "replit-objstore-005adaa9-1cce-4c34-9d82-43b2125a4ea5"

[userenv]

[userenv.shared]
VITE_FIREBASE_PROJECT_ID = "tpm-workflow"
